<?xml version="1.0" encoding="utf-8"?>
<package>
	<metadata>
		<id>Twilio.Conversations.Xamarin</id>
		<version>3.0.0</version>
		<authors>Twilio, Joseph Edusei</authors>
		<owners>jedusei</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/jedusei/Twilio.Conversations.Xamarin</projectUrl>
		<description>Xamarin.Android and Xamarin.iOS Bindings for the Twilio Conversation Android and iOS SDKs respectively.</description>
		<releaseNotes>This release includes bindings for the Twilio Conversations Android SDK (4.0.2) and iOS SDK (2.2.2).</releaseNotes>
		<tags>twilio conversations xamarin ios android</tags>
		<dependencies> 
			<group targetFramework="MonoAndroid44">
				<dependency id="Xamarin.AndroidX.Security.SecurityCrypto" version="1.0.0.8" />
				<dependency id="Xamarin.KotlinX.Coroutines.Android" version="1.6.4.1" />
				<dependency id="Xamarin.KotlinX.Coroutines.Core" version="1.6.4.1" />
				<dependency id="Xamarin.ReLinker" version="1.4.4" />
			</group>
			<group targetFramework="Xamarin.iOS10"/>
		</dependencies>
	</metadata>
	<files>
		<!-- Android -->
		<file src=".\Twilio.Conversations.Droid\bin\Release\Twilio.Conversations.Droid.*" target="lib\MonoAndroid44" />
		<file src=".\Twilio.Conversations.Droid\bin\Release\Twilsock.*" target="lib\MonoAndroid44" />
		<!-- iOS -->
		<file src=".\Twilio.Conversations.iOS\bin\Release\Twilio.Conversations.iOS.*" target="lib\Xamarin.iOS10" />
	</files>
</package>